<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="406.0" prefWidth="338.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.UpdatePasswordController">
    <children>
        <AnchorPane layoutY="75.0" prefHeight="333.0" prefWidth="338.0" style="-fx-background-color: #a6e3b6;">
            <children>
                <Label fx:id="l2" layoutX="38.0" layoutY="125.0" text="Set A NewPassword">
                    <font>
                        <Font name="Arial Black" size="14.0" />
                    </font>
                </Label>
                <Label fx:id="l21" layoutX="35.0" layoutY="210.0" text="Confirm Password">
                    <font>
                        <Font name="Arial Black" size="14.0" />
                    </font>
                </Label>
                <Label fx:id="l12" layoutX="38.0" layoutY="25.0" text="Enter Your Account Number">
                    <font>
                        <Font name="Arial Black" size="18.0" />
                    </font>
                </Label>
                <Button fx:id="submit" layoutX="255.0" layoutY="278.0" mnemonicParsing="false" onAction="#submitOnAction" prefWidth="60.0" text="Submit">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Button>
                <Button fx:id="cancel" layoutX="184.0" layoutY="278.0" mnemonicParsing="false" onAction="#cancelOnAction" prefWidth="60.0" text="Cancel">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Button>
                <TextField fx:id="accountNumber" alignment="CENTER" blendMode="MULTIPLY" layoutX="38.0" layoutY="52.0" prefHeight="25.0" prefWidth="277.0" promptText="AccountNumber" />
                <TextField fx:id="password" alignment="CENTER" blendMode="MULTIPLY" layoutX="36.0" layoutY="147.0" prefHeight="25.0" prefWidth="277.0" promptText="Password" />
                <TextField fx:id="confirmPassword" alignment="CENTER" blendMode="MULTIPLY" layoutX="35.0" layoutY="232.0" prefHeight="25.0" prefWidth="277.0" promptText="ConfirmPassword" />
                <Label fx:id="warning" layoutX="49.0" layoutY="87.0" prefWidth="250.0" textAlignment="CENTER" textFill="#e10707" underline="true">
                    <font>
                        <Font name="System Bold Italic" size="14.0" />
                    </font>
                </Label>
            </children>
        </AnchorPane>
        <Label fx:id="l121" layoutX="14.0" layoutY="14.0" prefHeight="40.0" prefWidth="313.0" text="Create An Internet Account" textFill="WHITE">
            <font>
                <Font name="Arial Black" size="20.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
